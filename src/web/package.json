{
    "private": true,
    "version": "0.0.0",
    "name": "@polymedia/iota-demo-web",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://iota-demo.polymedia.app",
    "description": "IOTA Demo",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/iota-demo"
    },
    "keywords": [],
    "scripts": {
        "build": "vite build",
        "clean": "rm -rf dist/ node_modules/ .turbo/ .wrangler/",
        "deploy-dev": "pnpm build && wrangler pages deploy dist/ --project-name iota-demo --branch dev",
        "deploy-prod": "pnpm build && wrangler pages deploy dist/ --project-name iota-demo --branch main",
        "dev": "vite",
        "host": "vite --host",
        "lint": "eslint src/ --report-unused-disable-directives --max-warnings 0",
        "preview": "pnpm build && vite preview",
        "typecheck": "tsc -b"
    },
    "dependencies": {
        "@iota/dapp-kit": "^0.4.0",
        "@iota/iota-sdk": "^0.5.0",
        "@polymedia/iota-demo-sdk": "workspace:*",
        "@tanstack/react-query": "^5.66.11",
        "normalize.css": "^8.0.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hot-toast": "^2.5.2",
        "react-router-dom": "^7.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@vitejs/plugin-react-swc": "^3.8.0",
        "less": "^4.2.2",
        "vite": "^6.2.0",
        "wrangler": "^3.111.0"
    },
    "type": "module",
    "sideEffects": false
}
